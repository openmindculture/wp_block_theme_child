				{
					"color": "#5aa4e1",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#044c88",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#12afa9",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#030f63",
					"name": "dark midnight blue",
					"slug": "dark-midnight-blue"
				},
				{
					"color": "#191970",
					"name": "midnight blue",
					"slug": "midnight-blue"
				},
				{
					"color": "#4b8402",
					"name": "soft grass green",
					"slug": "soft-grass-green"
				},
				{
					"color": "#e50691",
					"name": "dark deep pink",
					"slug": "dark-deep-pink"
